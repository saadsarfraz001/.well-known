<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/backgroundColor"
    android:padding="6dp"
    android:orientation="vertical"
    >


    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        card_view:cardBackgroundColor="#FFFFFF"
        card_view:cardCornerRadius="4dp"
        card_view:cardElevation="0dp"

        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="25dp"
            android:layout_marginStart="25dp"
            android:layout_marginBottom="10dp"
            android:orientation="vertical"

            >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <RatingBar
                    style="?android:attr/ratingBarStyleIndicator"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="0dp"
                    android:gravity="end"
                    android:isIndicator="true"
                    android:numStars="5"
                    android:progressTint="@color/averageColor"
                    android:rating="5"
                    android:scaleX="0.8"
                    android:scaleY="0.8"
                    android:secondaryProgressTint="@android:color/transparent" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginTop="1dp"
                    android:gravity="left|center"
                    android:text="@string/skill_title_1"
                    android:textSize="16sp"
                    app:fontFamily="@font/roboto"
                    card_view:ignore="RtlHardcoded" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <RatingBar
                    style="?android:attr/ratingBarStyleIndicator"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:isIndicator="true"
                    android:numStars="5"
                    android:progressTint="@color/averageColor"
                    android:rating="4"
                    android:scaleX="0.8"
                    android:scaleY="0.8"
                    android:secondaryProgressTint="@android:color/transparent" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="0dp"
                    android:gravity="left|center"
                    android:text="@string/skill_title_2"
                    android:textSize="16sp"
                    app:fontFamily="@font/roboto"
                    card_view:ignore="RtlHardcoded" />


            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <RatingBar
                    style="?android:attr/ratingBarStyleIndicator"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:isIndicator="true"
                    android:numStars="5"
                    android:progressTint="@color/averageColor"
                    android:rating="4"
                    android:scaleX="0.8"
                    android:scaleY="0.8"
                    android:secondaryProgressTint="@android:color/transparent"

                    />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="0dp"
                    android:gravity="left|center"
                    android:text="@string/skill_title_3"
                    android:textSize="16sp"
                    app:fontFamily="@font/roboto"
                    card_view:ignore="RtlHardcoded" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <RatingBar
                    style="?android:attr/ratingBarStyleIndicator"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_alignParentEnd="true"
                    android:isIndicator="true"
                    android:numStars="5"
                    android:progressTint="@color/averageColor"
                    android:rating="4"
                    android:scaleX="0.8"
                    android:scaleY="0.8"
                    android:secondaryProgressTint="@android:color/transparent"

                    />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="0dp"
                    android:gravity="left|center"
                    android:text="@string/skill_title_4"
                    android:textSize="16sp"
                    app:fontFamily="@font/roboto"
                    card_view:ignore="RtlHardcoded" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <RatingBar
                    style="?android:attr/ratingBarStyleIndicator"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_alignParentEnd="true"
                    android:isIndicator="true"
                    android:numStars="5"
                    android:progressTint="@color/averageColor"
                    android:rating="4"
                    android:scaleX="0.8"
                    android:scaleY="0.8"
                    android:secondaryProgressTint="@android:color/transparent"

                    />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="0dp"
                    android:layout_marginBottom="10dp"
                    android:gravity="left|center"
                    android:text="@string/skill_title_5"
                    android:textSize="16sp"
                    app:fontFamily="@font/roboto"
                    card_view:ignore="RtlHardcoded" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <RatingBar
                    style="?android:attr/ratingBarStyleIndicator"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_alignParentEnd="true"
                    android:isIndicator="true"
                    android:numStars="5"
                    android:progressTint="@color/averageColor"
                    android:rating="4"
                    android:scaleX="0.8"
                    android:scaleY="0.8"
                    android:secondaryProgressTint="@android:color/transparent"

                    />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="0dp"
                    android:layout_marginBottom="10dp"
                    android:gravity="left|center"
                    android:text="@string/skill_title_6"
                    android:textSize="16sp"
                    app:fontFamily="@font/roboto"
                    card_view:ignore="RtlHardcoded" />
            </RelativeLayout>

        </LinearLayout>


        <TextView
            android:layout_width="match_parent"
            android:layout_height="22dp"
            android:layout_marginStart="15dp"
            android:layout_marginTop="10dp"
            android:text="@string/hands_on_skills"
            android:textColor="@color/blackColor"
            android:textSize="18sp"
            app:fontFamily="@font/roboto" />


    </androidx.cardview.widget.CardView>





    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="6dp"
        card_view:cardBackgroundColor="#FFFFFF"
        card_view:cardCornerRadius="4dp"
        card_view:cardElevation="0dp"

        >


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginEnd="15dp"
            android:layout_marginStart="15dp"

            android:orientation="vertical">



            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="10dp"
                >


                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/projects_title"
                    android:textColor="@color/blackColor"
                    android:textSize="18sp"
                    app:fontFamily="@font/roboto"
                    card_view:ignore="MissingConstraints"
                    card_view:layout_editor_absoluteX="0dp"
                    card_view:layout_editor_absoluteY="9dp" />


                <TextView
                    android:id="@+id/projectName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="4dp"
                    android:text="@string/project_title"
                    android:textColor="@color/blackColor"
                    android:textSize="16sp"
                    app:fontFamily="@font/roboto"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView4"
                    card_view:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:text="@string/project_description"

                    android:textSize="14sp"
                    app:fontFamily="@font/roboto"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/projectName"
                    card_view:ignore="MissingConstraints" />

                <ImageView
                    android:id="@+id/githubCopyLink"
                    android:layout_width="20dp"
                    android:layout_height="20dp"

                    android:contentDescription="@string/todo"
                    android:src="@drawable/openlink"
                    app:layout_constraintBottom_toTopOf="@+id/textView"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toEndOf="@+id/projectName"
                    app:layout_constraintTop_toBottomOf="@+id/textView4"
                    app:layout_constraintVertical_bias="0.326"
                    card_view:ignore="MissingConstraints" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/grayColor"
                    app:layout_constraintTop_toBottomOf="@+id/textView"
                    android:layout_marginTop="10dp"
                    card_view:ignore="MissingConstraints"
                    card_view:layout_editor_absoluteX="16dp" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"

                >

                <TextView
                    android:id="@+id/projectName1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/project_title1"
                    android:textColor="@color/blackColor"
                    android:textSize="16sp"
                    android:layout_marginStart="10dp"
                    app:fontFamily="@font/roboto"
                    app:layout_constraintStart_toStartOf="parent"
                    card_view:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:text="@string/project_description1"
                    android:textSize="14sp"
                    app:fontFamily="@font/roboto"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/projectName1"
                    card_view:ignore="MissingConstraints" />

                <ImageView
                    android:id="@+id/githubCopyLin1"
                    android:layout_width="20dp"
                    android:layout_height="20dp"

                    android:contentDescription="@string/todo"
                    android:src="@drawable/openlink"
                    app:layout_constraintBottom_toTopOf="@+id/textView1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toEndOf="@+id/projectName1"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.064"
                    card_view:ignore="MissingConstraints" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/grayColor"
                    app:layout_constraintTop_toBottomOf="@+id/textView1"
                    android:layout_marginTop="10dp"
                    card_view:ignore="MissingConstraints"
                    card_view:layout_editor_absoluteX="16dp" />

            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"

                >

                <TextView
                    android:id="@+id/projectName2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/project_title3"
                    android:textColor="@color/blackColor"
                    android:textSize="16sp"
                    android:layout_marginStart="10dp"
                    app:fontFamily="@font/roboto"
                    app:layout_constraintStart_toStartOf="parent"
                    card_view:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:text="@string/project_description3"
                    android:textSize="14sp"
                    app:fontFamily="@font/roboto"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/projectName2"
                    card_view:ignore="MissingConstraints" />

                <ImageView
                    android:id="@+id/githubCopyLin2"
                    android:layout_width="20dp"
                    android:layout_height="20dp"

                    android:contentDescription="@string/todo"
                    android:src="@drawable/openlink"
                    app:layout_constraintBottom_toTopOf="@+id/textView2"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toEndOf="@+id/projectName2"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.064"
                    card_view:ignore="MissingConstraints" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>

    </androidx.cardview.widget.CardView>



</LinearLayout>
